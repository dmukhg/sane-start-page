{
  "manifest_version": 2,

  "name": "Sane Start Page",
  "description": "An aesthetic, productive start page.",
  "version" : "1.1",

  "permissions": [
    "bookmarks",
    "management",
    "chrome://favicon/"
  ],

  "icons" : {
    "128": "/images/icon-128.png",
    "64": "/images/icon-64.png",
    "32": "/images/icon-32.png",
  },

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
